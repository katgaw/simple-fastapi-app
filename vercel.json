{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://simple-fastapi-app-api.vercel.app/api/:path*"
    },
    {
      "source": "/recipe",
      "destination": "https://simple-fastapi-app-api.vercel.app/recipe"
    },
    {
      "source": "/health",
      "destination": "https://simple-fastapi-app-api.vercel.app/health"
    },
    {
      "source": "/docs",
      "destination": "https://simple-fastapi-app-api.vercel.app/docs"
    },
    {
      "source": "/openapi.json",
      "destination": "https://simple-fastapi-app-api.vercel.app/openapi.json"
    }
  ]
}

